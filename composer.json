{
    "name": "jumbojett/openid-connect-php",
    "description": "Bare-bones OpenID Connect client",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.4",
        "phpseclib/phpseclib": "~3.0.6",
        "ext-json": "*",
        "ext-curl": "*",
        "paragonie/random_compat": ">=2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.4",
        "roave/security-advisories": "dev-master"
    },
    "archive": {
        "exclude": [
            ".*"
        ]
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "config": {
        "platform": {
            "php": ">=7.4"
        }
    }
}